void main(){
        int i = 0;
        
        break;
        if(true) break;
        else     break;
        
        for(i = 0; i >= 5; ++i)
                break;
        for(i = 0; i == 5; ++i){
                break;
        }

        for(i = 0; i == 5; ++i){
            for(i = 0; i != 5; i--){
                if(false){
                        while(i <= i){
                                break;
                        }
                        break;
                }
            }
                break;
        }
        break;        
        { break; }
        
        
        while(i > 0)
                break;
        while(i != 0){
                break;
        }
        
        switch(i){
                case 5: break;
                case 6: {break;}
                case 7: if(true) break;
                default: break;
        }
}

